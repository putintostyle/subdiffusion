function y = BDF(y0, t0, dt, intTer, lambda, q, alpha)
%% This method use explicit method

    y = [y0];
    
    y_diff = [0];
    for time = 1:intTer

        f = source(t0+(time-1)*dt, alpha);
       
        if time == 1
            sol = (q(1)+lambda)*y(end);
        else
            sol = (q(1)+lambda)*y(end)-sum(ydiff.*q(2:time))+f;
        end
        y = [y, sol];
        y_diff = [ydiff, y(end)-y(end-1)];
    end
end

